{
	"name": "my-car",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "node index.js",
		"lint": "next lint",
		"prepare": "husky",
		"format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,mdx,css,yaml,yml}\"",
		"check": "prettier --check \"**/*.{ts,tsx,js,jsx,md,mdx,css,yaml,yml}\""
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged",
			"pre-push": "npm run build && git add ."
		}
	},
	"dependencies": {
		"next": "14.2.5",
		"react": "^18",
		"react-dom": "^18"
	},
	"devDependencies": {
		"@types/node": "^20",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"eslint": "^8",
		"eslint-config-next": "14.2.5",
		"husky": "^9.1.0",
		"postcss": "^8",
		"prettier": "^3.2.5",
		"pretty-quick": "^4.0.0",
		"tailwindcss": "^3.4.1",
		"typescript": "^5"
	}
}
